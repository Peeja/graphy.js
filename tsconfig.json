{
	"compilerOptions": {
		"lib": ["es2019", "es2020.promise", "es2020.bigint", "es2020.string", "es2020.symbol.wellknown"],
		"module": "commonjs",
		"target": "es2019",
		"moduleResolution": "node",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
}